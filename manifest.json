{
  "name": "kraMkooB",
  "description": "Hidden bookmarks, but made right. No usual username-password login, we introduce a new click password concept.",
  "version": "1.1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "permissions": ["activeTab", "tabs", "storage"],
  "incognito": "spanning",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/reverse-star.png"
    }
  },
  "icons": {
    "16": "/images/reverse-star.png"
  }
}
